{
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "description": "The asset id",
      "example": 1
    },
    "title": {
      "type": "string",
      "description": "The asset title",
      "example": "Asset 1"
    },
    "media_type": {
      "type": "string",
      "description": "The asset media type",
      "example": "video"
    },
    "thumbnails": {
      "$ref": "./Thumbnails.json"
    },
    "filename": {
      "type": "string",
      "description": "The asset filename",
      "example": "asset_1.mp4"
    },
    "extension": {
      "type": "string",
      "description": "The asset extension",
      "example": "mp4"
    },
    "created_at": {
      "type": "string",
      "description": "The asset update created at date",
      "example": "2020-01-01T00:00:00.000Z"
    },
    "position": {
      "type": "number",
      "description": "The asset position",
      "example": 1
    },
    "processed": {
      "type": "boolean",
      "description": "The asset processed status",
      "example": true
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "title",
    "media_type",
    "thumbnails",
    "filename",
    "created_at",
    "extension",
    "processed"
  ]
}